<!DOCTYPE html>
<html>
<head>
  <title>Resilience Project - Login</title>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"
  integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u"
  crossorigin="anonymous">
</head>

<body>

  <div>
    <p><a href = "resilienceMain">Back to Resilience home</a>.</p>
  </div>

  <h1> Patient List: </h1>

    <h3> Unreviewed: </h3>
    <ul>
      <% for(i = 0; i < list.length; i++) { %>
          <li>
            <input type="text" name="pun_name" id="<%= 'pun_name'+i %>"
            readonly value="<%= list[i].FirstName %> <%= list[i].LastName %>">
          </li>

      <% } %>
    </ul>

    <h3> Accepted: </h3>
    <ul>
      <% for(i = 0; i < list1.length; i++) { %>
          <li>
            <input type="text" name="pacc_name" id="<%= 'pacc_name'+i %>"
            readonly value="<%= list1[i].FirstName %> <%= list1[i].LastName %>">
          </li>

      <% } %>
    </ul>


</body>
</html>
